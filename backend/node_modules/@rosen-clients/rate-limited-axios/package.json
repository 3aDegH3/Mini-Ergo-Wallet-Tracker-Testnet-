{
  "name": "@rosen-clients/rate-limited-axios",
  "version": "0.1.0",
  "description": "A lightweight rate-limiting wrapped Axios that helps control the number of HTTP requests and prevent API throttling.",
  "license": "MIT",
  "author": "Rosen Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint --fix . && npm run prettify",
    "prettify": "prettier --write . --ignore-path ./.gitignore",
    "release": "npm run test -- --run && npm run build && npm publish --access public",
    "test": "NODE_OPTIONS=--loader=extensionless vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-istanbul": "^2.0.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "extensionless": "^1.9.6",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "dependencies": {
    "@rosen-bridge/abstract-logger": "^2.0.1",
    "await-semaphore": "^0.1.3",
    "axios": "^1.9.0"
  }
}
